<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="root" minHeight="588.0" minWidth="960.0" prefHeight="588.0" prefWidth="960.0" style="-fx-background-color:#FFFFFF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <!-- Header -->
        <BorderPane>
            <left>
                <VBox alignment="TOP_CENTER" minWidth="72.0" prefWidth="72.0" spacing="24.0" BorderPane.alignment="TOP_CENTER">
                    <children>
                        <ImageView fx:id="iv_user" fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                            <VBox.margin>
                                <Insets top="18.0" />
                            </VBox.margin>
                            <image>
                                <Image url="@/image/ic_user.png" />
                            </image>
                        </ImageView>
                        <VBox fx:id="vb_device" alignment="CENTER" minWidth="72.0" prefWidth="72.0">
                            <children>
                                <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@/image/ic_device_color.png" />
                                    </image>
                                </ImageView>
                                <Label style="-fx-text-fill:#2979ff;-fx-font-size:8;" text="Camera" />
                            </children>
                        </VBox>
                        <VBox fx:id="vb_quad" alignment="CENTER" minWidth="72.0" prefWidth="72.0">
                            <children>
                                <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@/image/ic_quad_screen_mid.png" />
                                    </image>
                                </ImageView>
                                <Label style="-fx-text-fill:#95a5ad;-fx-font-size:8;" text="Quad screen" />
                            </children>
                        </VBox>
                        <VBox fx:id="vb_nine" alignment="CENTER" minWidth="72.0" prefWidth="72.0">
                            <children>
                                <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@/image/ic_nine_screen_mid.png" />
                                    </image>
                                </ImageView>
                                <Label style="-fx-text-fill:#95a5ad;-fx-font-size:8;" text="Nine-screen" />
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </left>

            <!-- Content Area -->
            <center>
            </center>
         <StackPane.margin>
            <Insets top="48.0" />
         </StackPane.margin>
        </BorderPane>
    </children>
</StackPane>
