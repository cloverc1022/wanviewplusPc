<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER_LEFT">
            <children>
                <ImageView fx:id="icon" fitHeight="48.0" fitWidth="48.0" layoutX="32.0" layoutY="50.0"
                           pickOnBounds="true" preserveRatio="true">
                    <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </HBox.margin>
                </ImageView>
                <VBox alignment="CENTER_RIGHT" layoutX="384.0" layoutY="100.0">
                    <children>
                        <Label fx:id="deviceName" maxWidth="120.0" minWidth="120.0" prefWidth="120.0"
                               styleClass="devices_item_deviceName" text="Name">
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </Label>
                        <Label fx:id="status" styleClass="devices_item_status" text="Connecting" alignment="CENTER"/>
                    </children>
                    <HBox.margin>
                        <Insets right="12.0"/>
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
