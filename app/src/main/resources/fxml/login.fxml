                          minHeight="64.0" prefHeight="64.0" promptText="Password" unFocusColor="#acbec6"
                          AnchorPane.leftAnchor="64.0" AnchorPane.rightAnchor="64.0" AnchorPane.topAnchor="418.0"
                          style="-fx-text-fill:#223239;-fx-background-color:#FFFFFFFF;-fx-font-size:28px;"/>
        <JFXButton text="Sign in" buttonType="RAISED" layoutX="266.0" layoutY="579.0" maxHeight="80.0" minHeight="80.0"
                   prefHeight="80.0" textFill="WHITE"
                   AnchorPane.bottomAnchor="74.0" AnchorPane.leftAnchor="64.0" AnchorPane.rightAnchor="64.0"
                   onMouseClicked="#handleSubmitButtonAction"
                   style="-fx-text-fill:WHITE;-fx-background-color:#0078ff;-fx-font-size:32px;">
        </JFXButton>
    </children>
</AnchorPane>
