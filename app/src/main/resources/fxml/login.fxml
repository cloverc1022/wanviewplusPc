<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:controller="net.ajcloud.wansviewplusw.login.LoginController" maxHeight="720.0" maxWidth="640.0"
            minHeight="720.0" minWidth="640.0" prefHeight="720.0" prefWidth="640.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            style="-fx-background-color:#FFFFFF;">
    <children>
        <ImageView fitHeight="96.0" fitWidth="96.0" layoutX="272.0" layoutY="60.0"
                   pickOnBounds="true"
                   preserveRatio="true" AnchorPane.leftAnchor="272.0" AnchorPane.topAnchor="32.0">
            <image>
                <Image url="@/image/ic_logo.png"/>
            </image>
        </ImageView>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="114.0" layoutY="152.0" text="Welcome to Wansview"
               textAlignment="CENTER" wrapText="true" AnchorPane.leftAnchor="114.0"
               AnchorPane.rightAnchor="113.0" AnchorPane.topAnchor="160.0"
               style="-fx-text-fill:#223138;-fx-background-color:#FFFFFFFF;-fx-font-size:36;">
        </Label>
        <JFXTextField fx:id="tf_name" focusColor="#0078ff" labelFloat="true" layoutX="173.0" layoutY="286.0"
                      maxHeight="64.0"
                      minHeight="64.0" prefHeight="64.0" promptText="Email" unFocusColor="#acbec6"
                      AnchorPane.leftAnchor="64.0" AnchorPane.rightAnchor="64.0" AnchorPane.topAnchor="275.0"
                      style="-fx-text-fill:#223239;-fx-background-color:#FFFFFFFF;-fx-font-size:28px;">
        </JFXTextField>
        <JFXPasswordField fx:id="tf_password" focusColor="#0078ff" labelFloat="true" layoutX="174.0" layoutY="449.0"
