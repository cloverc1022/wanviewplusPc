<?xml version="1.0" encoding="UTF-8"?>
<project name="module_wansviewcloud_win" default="compile.module.wansviewcloud_win">
  <dirname property="module.wansviewcloud_win.basedir" file="${ant.file.module_wansviewcloud_win}"/>
  
  <property name="module.jdk.home.wansviewcloud_win" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.wansviewcloud_win" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.wansviewcloud_win" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.wansviewcloud_win" value="-encoding UTF-8 -source 10 -target 10 ${compiler.args}"/>
  
  <property name="wansviewcloud_win.output.dir" value="${module.wansviewcloud_win.basedir}/out/production/WansViewCloud_win"/>
  <property name="wansviewcloud_win.testoutput.dir" value="${module.wansviewcloud_win.basedir}/out/test/WansViewCloud_win"/>
  
  <path id="wansviewcloud_win.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="wansviewcloud_win.module.production.classpath">
    <path refid="${module.jdk.classpath.wansviewcloud_win}"/>
  </path>
  
  <path id="wansviewcloud_win.runtime.production.module.classpath">
    <pathelement location="${wansviewcloud_win.output.dir}"/>
  </path>
  
  <path id="wansviewcloud_win.module.classpath">
    <path refid="${module.jdk.classpath.wansviewcloud_win}"/>
    <pathelement location="${wansviewcloud_win.output.dir}"/>
  </path>
  
  <path id="wansviewcloud_win.runtime.module.classpath">
    <pathelement location="${wansviewcloud_win.testoutput.dir}"/>
    <pathelement location="${wansviewcloud_win.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.wansviewcloud_win">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.wansviewcloud_win">
    <patternset refid="excluded.from.module.wansviewcloud_win"/>
  </patternset>
  
  <path id="wansviewcloud_win.module.sourcepath">
    <dirset dir="${module.wansviewcloud_win.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.wansviewcloud_win" depends="compile.module.wansviewcloud_win.production,compile.module.wansviewcloud_win.tests" description="Compile module WansViewCloud_win"/>
  
  <target name="compile.module.wansviewcloud_win.production" depends="register.custom.compilers" description="Compile module WansViewCloud_win; production classes">
    <mkdir dir="${wansviewcloud_win.output.dir}"/>
    <javac2 destdir="${wansviewcloud_win.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.wansviewcloud_win}/javac">
      <compilerarg line="${compiler.args.wansviewcloud_win}"/>
      <bootclasspath refid="wansviewcloud_win.module.bootclasspath"/>
      <classpath refid="wansviewcloud_win.module.production.classpath"/>
      <src refid="wansviewcloud_win.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.wansviewcloud_win"/>
    </javac2>
    
    <copy todir="${wansviewcloud_win.output.dir}">
      <fileset dir="${module.wansviewcloud_win.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.wansviewcloud_win.tests" depends="register.custom.compilers,compile.module.wansviewcloud_win.production" description="compile module WansViewCloud_win; test classes" unless="skip.tests"/>
  
  <target name="clean.module.wansviewcloud_win" description="cleanup module">
    <delete dir="${wansviewcloud_win.output.dir}"/>
    <delete dir="${wansviewcloud_win.testoutput.dir}"/>
  </target>
</project>